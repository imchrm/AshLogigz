package com.chrm.logigz.componets
{
	public class DPos
	{
		public var x0:Number = 0;
		public var y0:Number = 0;
		public var x1:Number = 0;
		public var y1:Number = 0;
		
		public function DPos(x0 : Number = 0, y0 : Number = 0, x1 : Number = 0, y1 : Number = 0)
		{
			this.x0 = x0;
			this.y0 = y0;
			this.x1 = x1;
			this.y1 = y1;
		}
	}
}
